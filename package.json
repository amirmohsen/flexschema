{
  "name": "flexschema",
  "version": "0.1.0",
  "repository": "https://github.com/amirmohsen/flexschema",
  "description": "A highly flexible and dynamic schema library",
  "scripts": {
    "build": "rollup -c -m",
    "dev": "rollup -c -m -w",
    "test": "jest",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "main": "dist/flexschema.cjs.js",
  "module": "dist/flexschema.esm.js",
  "browser": "dist/flexschema.umd.js",
  "keywords": [
    "flexverse",
    "schema"
  ],
  "author": {
    "name": "Amir Abdolrazaghi",
    "email": "amir@flexverse.tech"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amirmohsen/flexschema/issues"
  },
  "homepage": "https://github.com/amirmohsen/flexschema#readme",
  "babel": {
    "presets": [
      [
        "env"
      ]
    ],
    "plugins": [
      "syntax-decorators",
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "clone": "^2.1.1",
    "common-tags": "^1.4.0",
    "es6-error": "^4.0.2",
    "extend": "^3.0.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.4",
    "object-path": "^0.11.4",
    "object-path-immutable": "^0.5.1",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "jest": "^21.2.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  }
}
